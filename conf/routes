GET     /artists/keyword/:keyword                       controllers.ArtistsController.getArtistsByKeyword(keyword : String)
GET     /artists/:offset/:count                         controllers.ArtistsController.getAll(offset: Integer, count: Integer)
GET     /artists/:id                                    controllers.ArtistsController.get(id: Long)
PUT     /artists/:id                                    controllers.ArtistsController.update(id: Long, request: Request)

