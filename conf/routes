GET     /movies/dashboard                               controllers.MoviesController.dashboard()
POST    /movies/filter                                  controllers.MoviesController.filter(request: Request)
POST    /movies                                         controllers.MoviesController.add(request: Request)
GET     /movies/:id                                     controllers.MoviesController.get(id: Long)
PUT     /movies/:id                                     controllers.MoviesController.update(id: Long, request: Request)

POST    /artists                                        controllers.ArtistsController.addArtist(request: Request)
GET     /artists/:offset/:count                         controllers.ArtistsController.getAll(offset: Integer, count: Integer)
GET     /artists/:id                                    controllers.ArtistsController.get(id: String)
PUT     /artists/:id                                    controllers.ArtistsController.update(id: String, request: Request)

GET     /languages                                      controllers.LanguageController.getAll

GET     /formats                                        controllers.FormatController.getAll